<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.itcast.elec.domain.ElecSystemDDL" table="Elec_SystemDDL">
        <id name="seqID" type="integer" column="seqID">
            <generator class="increment" />
        </id>
        <property name="keyword" type="string" column="keyword" />
        <property name="ddlCode" type="integer" column="ddlCode"/>
        <property name="ddlName" type="string" column="ddlName"/>
    </class>
</hibernate-mapping>
